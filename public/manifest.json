{
  "manifest_version": 2,
  "name": "Startup Page of the Day",
  "description" : "Startup pages based on the day of the week",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "index.html",
  "browser_action": {
    "default_icon": "./images/icon.png"
  },
  "permissions": [
    "background",
    "tabs",
    "storage",
    "bookmarks"
  ]
}

